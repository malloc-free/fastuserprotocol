#!/bin/bash

export LD_LIBRARY_PATH=lib

if test "$1" == "tcp"
then
	./TestBed client 192.168.0.9 4950 tcp 1408 size 1048576
elif test "$1" == "udt"
then
	./TestBed mclient 192.168.0.9 4950 udt 1408 size 32768
elif test "$1" == "utp"
then
	./TestBed client 192.168.0.9 4950 utp 1408 size 1048576
elif test "$1" == "dccp"
then
	./TestBed mclient 192.168.0.9 4950 dccp 0 size 1048576
elif test "$1" == "udp"
then
	./TestBed mclient 192.168.0.9 4950 udp 1408 size 1048576
elif test "$1" == "eudp"
then
	./TestBed client 192.168.0.9 4950 eudp 1408 size 1048576
else
	echo "No such protocol"
fi
